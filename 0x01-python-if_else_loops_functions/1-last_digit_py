#!/usr/bin/python3
import random
number = random.randint(-10000, 10000)
if int(repr(number)[-1]) > 5:
    str = "and is greater than 5"
elif int(repr(number)[-1]) == 0:
    str = "and is zero"
else:
    str = "and is less than 6 and not 0"
print("Last digit of", number, "is", int(repr(number)[-1]), str)
